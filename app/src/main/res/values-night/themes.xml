<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Adventours" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
    </style>

    <style name="SCREEN">
        <item name="android:background">#252525</item>
    </style>

    <style name="ACTION">
        <item name="android:background">#1D636C</item>
    </style>

    <style name="HEADER">
        <item name="android:background">#1E636C</item>
    </style>

    <style name="PRICETEXT">
        <item name="android:textColor">#B3B7BD</item>
    </style>

    <style name="CARD">
        <item name="android:background">#4B4F4F</item>
    </style>

    <style name="VIEWDETAILBTN">
        <item name="android:background">@drawable/white_button</item>
        <item name="android:textColor">#4B4F4F</item>
    </style>

    <style name="SEARCH">
        <item name="android:background">@drawable/dark_round_padding</item>
        <item name="android:textColor">#4B4F4F</item>
        <item name="android:drawableTint">#4B4F4F</item>
    </style>

    <style name="TEXT">
        <item name="android:textColor">#B3B7BD</item>
        <item name="android:drawableTint">@color/cyan</item>
    </style>

    <style name="LINE">
        <item name="android:background">#B3B7BD</item>
    </style>

    <style name="HINT">
        <item name="android:textColorHint">#B3B7BD</item>
    </style>

    <style name="PLACE">
        <item name="android:textColor">@color/cyan</item>
        <item name="android:drawableTint">@color/cyan</item>
    </style>

    <style name="BUTTON">
        <item name="android:background">@drawable/buttondarkmode</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="NAVBAR">
        <item name="android:background">#1D636C</item>
    </style>
</resources>